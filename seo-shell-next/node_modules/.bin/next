#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/next@15.0.7_@babel+core@7.28.5_babel-plugin-macros@3.1.0_babel-plugin-react-compiler@1.0.0_re_jdwzmpi7beqrgxz6ubpc2vgsui/node_modules/next/dist/bin/node_modules:/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/next@15.0.7_@babel+core@7.28.5_babel-plugin-macros@3.1.0_babel-plugin-react-compiler@1.0.0_re_jdwzmpi7beqrgxz6ubpc2vgsui/node_modules/next/dist/node_modules:/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/next@15.0.7_@babel+core@7.28.5_babel-plugin-macros@3.1.0_babel-plugin-react-compiler@1.0.0_re_jdwzmpi7beqrgxz6ubpc2vgsui/node_modules/next/node_modules:/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/next@15.0.7_@babel+core@7.28.5_babel-plugin-macros@3.1.0_babel-plugin-react-compiler@1.0.0_re_jdwzmpi7beqrgxz6ubpc2vgsui/node_modules:/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/next@15.0.7_@babel+core@7.28.5_babel-plugin-macros@3.1.0_babel-plugin-react-compiler@1.0.0_re_jdwzmpi7beqrgxz6ubpc2vgsui/node_modules/next/dist/bin/node_modules:/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/next@15.0.7_@babel+core@7.28.5_babel-plugin-macros@3.1.0_babel-plugin-react-compiler@1.0.0_re_jdwzmpi7beqrgxz6ubpc2vgsui/node_modules/next/dist/node_modules:/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/next@15.0.7_@babel+core@7.28.5_babel-plugin-macros@3.1.0_babel-plugin-react-compiler@1.0.0_re_jdwzmpi7beqrgxz6ubpc2vgsui/node_modules/next/node_modules:/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/next@15.0.7_@babel+core@7.28.5_babel-plugin-macros@3.1.0_babel-plugin-react-compiler@1.0.0_re_jdwzmpi7beqrgxz6ubpc2vgsui/node_modules:/Users/guibais/Documents/projects/severinos/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../next/dist/bin/next" "$@"
fi
